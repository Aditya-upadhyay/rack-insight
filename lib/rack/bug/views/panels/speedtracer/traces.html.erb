<h3>Traces</h3>
<% unless advice.empty? %>
  <p>Warning:</p>
  <ul>
    <% advice.each do |item| %>
      <li><%= item %></li>
    <% end %>
  </ul>
<% end %>
<dl>
  <% traces.each do |uuid, trace| %>
    <dt><%= uuid %> - <%= trace.start.strftime("%H:%M:%S") %></dt>
    <dd><%= trace.to_html %></dd>
  <% end %>
</dl>
